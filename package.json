{
  "name": "guess-melody",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve --copy",
    "build": "vue-cli-service build --modern --report",
    "test": "vue-cli-service test:unit -R nyan",
    "lint": "vue-cli-service lint",
    "help": "vue-cli-service help",
    "inspect": "vue-cli-service inspect",
    "precommit": "npm run lint & npm run test"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "register-service-worker": "^1.6.2",
    "vue": "^2.6.10",
    "vue-clickaway": "^2.2.2",
    "vue-i18n": "^8.9.0",
    "vue-i18n-babelfish": "^1.0.1",
    "vue-router": "^3.0.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.5.0",
    "@vue/cli-plugin-eslint": "^3.5.0",
    "@vue/cli-plugin-unit-mocha": "^3.5.0",
    "@vue/cli-service": "^3.5.3",
    "@vue/eslint-config-airbnb": "^4.0.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.15.3",
    "eslint-plugin-vue": "^5.0.0",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "vue-template-compiler": "^2.6.10"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "@vue/airbnb"
    ],
    "rules": {
      "no-param-reassign": "off",
      "import/extensions": "off",
      "key-spacing": "off",
      "no-multi-spaces": "off"
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "homepage": "https://redvi.github.io"
}
